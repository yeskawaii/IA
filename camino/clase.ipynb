   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "img = cv2.imread('carretera.jpg',0)\n",
    "\n",
    "w,h=img.shape\n",
    "for i in range (w):\n",
    "    for j in range (h):\n",
    "        img[i,j]= 255-img[i,j]\n",
    "\n",
    "cv2.imshow('Carretera con filtro de color', img)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
